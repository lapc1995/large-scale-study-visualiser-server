<div class="grid grid-cols-[30px_repeat(auto-fill,_30px)] gap-1 p-4">
    <% for(var i =0; i < finishedCounters; i++) { %>
        <%- include('includes/countElementDone') %>
    <% } %>
    <%- include('includes/countElementInProgress') %>
    <% for(var i=0; i < unfinishedCounters-1; i++) { %>
        <%- include('includes/countElementWaiting') %>
    <% } %>
</div>

<div class="text-2xl p-4 text-center"><%= finishedCounters %>.000 / 1.000.000</div>

